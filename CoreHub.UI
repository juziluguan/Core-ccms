local UserInputService = game:GetService("UserInputService")
local isUIVisible = true

local function toggleUI()
    if J.UIElements and J.UIElements.Main then
        local mainFrame = J.UIElements.Main
        isUIVisible = not isUIVisible
        mainFrame.Visible = isUIVisible
        if J.Toggle then
            J:Toggle()
        end
    elseif J:FindFirstChild("Main") then
        local mainFrame = J.Main
        isUIVisible = not isUIVisible
        mainFrame.Visible = isUIVisible
    end
end

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed then
        if input.KeyCode == Enum.KeyCode.Semicolon then
            toggleUI()
        end
    end
end)

task.spawn(function()
    local mainContainer
    if J.UIElements and J.UIElements.Main then
        mainContainer = J.UIElements.Main
    elseif J:FindFirstChild("Main") then
        mainContainer = J.Main
    end

    if mainContainer then

        mainContainer.BackgroundColor3 = Color3.new(0,0,0)
        mainContainer.BackgroundTransparency = 0

        local stroke = Instance.new("UIStroke")
        stroke.Name = "GreenStroke"
        stroke.Thickness = 3
        stroke.Color = Color3.new(1, 1, 1)
        stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

        local gradient = Instance.new("UIGradient")
        gradient.Name = "GreenGradient"
        gradient.Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 255, 0)),    
            ColorSequenceKeypoint.new(0.3, Color3.fromRGB(0, 100, 0)),  
            ColorSequenceKeypoint.new(0.7, Color3.fromRGB(0, 200, 0)),  
            ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 0))     
        })
        gradient.Enabled = true
        gradient.Offset = Vector2.new(0, 0)

        gradient.Parent = stroke
        stroke.Parent = mainContainer

        task.spawn(function()
            local rotationSpeed = 40
            while mainContainer and mainContainer.Parent do
                task.wait(0.01)
                gradient.Rotation = (gradient.Rotation + rotationSpeed * 0.1) % 360
            end
        end)
    end
end)

task.spawn(function()
    task.wait(1)

    local screenGui = F.ScreenGui

    local blurEffect = Instance.new("BlurEffect")
    blurEffect.Size = 0
    blurEffect.Name = "WindUIMainBlur"

    local depthOfField = Instance.new("DepthOfFieldEffect")
    depthOfField.FarIntensity = 0
    depthOfField.InFocusRadius = 0.1
    depthOfField.NearIntensity = 1
    depthOfField.Name = "WindUIDepthOfField"

    local lighting = game:GetService("Lighting")

    local originalEffects = {}

    local function enableBlur()
        for _, effect in pairs(lighting:GetChildren()) do
            if effect:IsA("BlurEffect") or effect:IsA("DepthOfFieldEffect") then
                originalEffects[effect] = {
                    Enabled = effect.Enabled,
                    Parent = effect.Parent
                }
                effect.Enabled = false
            end
        end

        if game:GetService("Workspace").CurrentCamera then
            for _, effect in pairs(game:GetService("Workspace").CurrentCamera:GetChildren()) do
                if effect:IsA("BlurEffect") or effect:IsA("DepthOfFieldEffect") then
                    originalEffects[effect] = {
                        Enabled = effect.Enabled,
                        Parent = effect.Parent
                    }
                    effect.Enabled = false
                end
            end
        end

        depthOfField.Parent = lighting
        blurEffect.Parent = lighting

        local tweenService = game:GetService("TweenService")
        local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        local blurTween = tweenService:Create(blurEffect, tweenInfo, {Size = 10})
        blurTween:Play()
    end

    local function disableBlur()
        for effect, data in pairs(originalEffects) do
            if effect and effect.Parent then
                effect.Enabled = data.Enabled
            end
        end

        local tweenService = game:GetService("TweenService")
        local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
        local blurTween = tweenService:Create(blurEffect, tweenInfo, {Size = 0})
        blurTween:Play()

        blurTween.Completed:Connect(function()
            blurEffect.Parent = nil
            depthOfField.Parent = nil
        end)
    end

    if J then
        if J.UIElements and J.UIElements.Main then
            J.UIElements.Main:GetPropertyChangedSignal("Visible"):Connect(function()
                if J.UIElements.Main.Visible then
                    enableBlur()
                else
                    disableBlur()
                end
            end)
        end

        J.Destroying:Connect(function()
            disableBlur()
        end)
    end

    if J and J.UIElements and J.UIElements.Main and J.UIElements.Main.Visible then
        enableBlur()
    end
end)

